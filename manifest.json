{
    "manifest_version": 3,
    "name": "Brainrot Shield Test",
    "version": "1.0",
    "description": "Testing our first extension",
    "action": {
      "default_popup": "hello.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["images/*.gif", "images/*.png", "images/*.jpg", "popup.js"],
            "matches": ["<all_urls>"]
        }
    ]
}